
.v-bottomup {
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 100;
  overflow: hidden;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}
.v-bottomup-header {
  padding: 30rpx;
  font-weight: bold;
  font-size: 40rpx;
}
.v-bottomup-title {
  display: inline-block !important;
  font-size: inherit !important;
}
.v-bottomup-close {
  float: right;
  padding-left: 30rpx;
  font-weight: normal;
}
.v-bottomup-wrap {
  position: absolute;
  bottom: 0;
  left: 0;
  z-index: 2;
  overflow: hidden;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  max-height: 100%;
  width: 100%;
  height: auto;
  outline: none;
  -webkit-transform: none;
  -ms-transform: none;
      transform: none;
}
.v-bottomup-content {
  overflow: hidden;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  border-radius: 0;
  background: #fff;
}
.v-bottomup-content.v-bottomup-transparent {
  background: transparent !important;
}
.v-bottomup-mask {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 1;
  background-color: rgba(0, 0, 0, 0.4);
}
.v-bottomup-open .v-bottomup-content {
  -webkit-animation: slideInUp 0.4s;
  animation: slideInUp 0.4s;
  -webkit-animation-fill-mode: forwards;
  animation-fill-mode: forwards;
}
.v-bottomup-close .v-bottomup-content {
  -webkit-animation: slideOutDown 0.4s;
  animation: slideOutDown 0.4s;
  -webkit-animation-fill-mode: forwards;
  animation-fill-mode: forwards;
}
.v-bottomup-open .v-bottomup-mask {
  -webkit-animation: fadeIn 0.4s ease 0s;
  animation: fadeIn 0.4s ease 0s;
  -webkit-animation-fill-mode: both;
  animation-fill-mode: both;
}
.v-bottomup-close .v-bottomup-mask {
  -webkit-animation: fadeOut 0.4s;
  animation: fadeOut 0.4s;
  -webkit-animation-fill-mode: forwards;
  animation-fill-mode: forwards;
}
.v-icon-bottom-close {
  float: right;
  padding-left: 30rpx;
  font-weight: normal;
}
@-webkit-keyframes fadeIn {
  from {
    opacity: 0;
  }
  to {
    opacity: 1;
  }
}
@keyframes fadeIn {
  from {
    opacity: 0;
  }
  to {
    opacity: 1;
  }
}
@-webkit-keyframes fadeOut {
  from {
    opacity: 1;
  }
  to {
    opacity: 0;
  }
}
@keyframes fadeOut {
  from {
    opacity: 1;
  }
  to {
    opacity: 0;
  }
}
@-webkit-keyframes slideInUp {
  from {
    visibility: visible;
    -webkit-transform: translate3d(0, 100%, 0);
    transform: translate3d(0, 100%, 0);
  }
  to {
    -webkit-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0);
  }
}
@keyframes slideInUp {
  from {
    visibility: visible;
    -webkit-transform: translate3d(0, 100%, 0);
    transform: translate3d(0, 100%, 0);
  }
  to {
    -webkit-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0);
  }
}
@-webkit-keyframes slideOutDown {
  from {
    -webkit-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0);
  }
  to {
    visibility: hidden;
    -webkit-transform: translate3d(0, 100%, 0);
    transform: translate3d(0, 100%, 0);
  }
}
@keyframes slideOutDown {
  from {
    -webkit-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0);
  }
  to {
    visibility: hidden;
    -webkit-transform: translate3d(0, 100%, 0);
    transform: translate3d(0, 100%, 0);
  }
}
