
.v-keyboard {
  position: relative;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  height: 470rpx;
  background-color: #fff;
}
.v-keyboard-container {
  height: 472rpx;
}
.v-keyboard-grid {
  overflow: hidden;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  border-top: 1rpx solid #ddd;
  font-weight: 600;
  font-size: 32rpx;
}
.v-keyboard-grid .v-keyboard-item:nth-child(3n+2) {
  width: 34%;
}
.v-keyboard-item {
  float: left;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  width: 33%;
  height: 100rpx;
  border-bottom: 1rpx solid #ddd;
  border-left: 1rpx solid #ddd;
  text-align: center;
  line-height: 100rpx;
}
.v-keyboard-item:nth-child(3n+1) {
  border-left: none;
}
.v-keyboard-item:active {
  background-color: #eaeaea;
}
.v-keyboard-item-c {
  background: url("data:image/png;base64,") no-repeat center center;
  background-size: 52.5rpx 40.5rpx;
  text-indent: -999999rpx;
}
.v-keyboard-item-f {
  background-color: #ff5346;
  color: #fff;
}
.v-keyboard-item-f:active {
  background-color: #ce3636;
}
.v-keyboard-item-f.v-keyboard-disabled {
  background-color: #fff !important;
  color: transparent !important;
}
.v-keyboard-conversion {
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  text-align: center;
  font-size: 22rpx;
  line-height: 70rpx;
}
.v-keyboard-conversion:active {
  background-color: #eaeaea;
}
