<block wx:if="{{$taroCompReady}}">
    <block wx:if="{{!visible}}">
        <view></view>
    </block>
    <block wx:else="{{true}}">
        <view class="v-class v-noticebar">
            <my-icon v-class="v-noticebar-notice" compid="{{$compid__16}}"></my-icon>
            <view class="v-noticebar-wrap">
                <view class="{{anonymousState__temp}}" animation="{{animationData}}">
                    <slot></slot>
                </view>
            </view>
            <block>
                <block wx:if="{{closable}}">
                    <my-icon v-class="v-noticebar-close" compid="{{$compid__17}}"></my-icon>
                </block>
                <block wx:else>
                    <view class="v-noticebar-gap"></view>
                </block>
            </block>
        </view>
    </block>
</block>