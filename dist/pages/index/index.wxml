<block wx:if="{{$taroCompReady}}">
    <scroll-view class="index" scroll-y="{{true}}" style="{{anonymousState__temp}}" scroll-into-view="{{scrollIntoView}}" scroll-with-animation="{{true}}">
        <view class="swiper-wap">
            <swiper indicator-dots="{{false}}" autoplay="{{true}}" circular="{{true}}" bindchange="changeSwiperIndex">
                <swiper-item>
                    <image class="swiper-image" mode="scaleToFill" src="https://file.40017.cn/css40017cnproduct/jinrong/files/i/ttb/open/bg-1.png"></image>
                </swiper-item>
                <swiper-item>
                    <image class="swiper-image" mode="scaleToFill" src="https://file.40017.cn/css40017cnproduct/jinrong/files/i/baoxian/hybrid/home/dd-1.png"></image>
                </swiper-item>
            </swiper>
            <view class="swiper-dots">
                <view class="{{em.$loopState__temp3}}" wx:key="$loopState__temp5" wx:for="{{loopArray7}}" wx:for-item="em" wx:for-index="index"></view>
            </view>
        </view>
        <view class="portal-list">
            <view class="portal-item" bindtap="scrollToElement" data-e-tap-so="this" data-e-tap-a-a="introduction">
                <view class="portal-item-icon pi-icon-1"></view><text class="portal-item-text">公司简介</text>
            </view>
            <view class="portal-item" bindtap="scrollToElement" data-e-tap-so="this" data-e-tap-a-a="production">
                <view class="portal-item-icon pi-icon-2"></view><text class="portal-item-text">产品介绍</text>
            </view>
            <view class="portal-item" bindtap="scrollToElement" data-e-tap-so="this" data-e-tap-a-a="success">
                <view class="portal-item-icon pi-icon-3"></view><text class="portal-item-text">成功案例</text>
            </view>
            <view class="portal-item" bindtap="scrollToElement" data-e-tap-so="this" data-e-tap-a-a="contact">
                <view class="portal-item-icon pi-icon-4"></view><text class="portal-item-text">联系我们</text>
            </view>
        </view>
        <view class="empty-padding ep-small"></view>
        <notice-bar v-class="index-notice" v-noticebar-notice="index-noticebar-notice" compid="{{$compid__92}}">认真提供技术服务，提供各领域专业的解决方案，打通线上线下营销场景</notice-bar>
        <view class="empty-padding"></view>
        <view class="block" id="introduction">
            <view class="block-head">
                <view class="iff-icon left"></view><text class="bh-word">公司简介</text>
                <view class="iff-icon right"></view>
            </view>
            <view>
                <image class="block-image" mode="center" src="https://file.40017.cn/css40017cnproduct/jinrong/files/i/baoxian/hybrid/home/sd-1.png"></image>
            </view>
        </view>
        <view class="empty-padding"></view>
        <view class="block" id="production">
            <view class="block-head">
                <view class="iff-icon left"></view><text class="bh-word">主要产品</text>
                <view class="iff-icon right"></view>
            </view>
            <view class="empty-padding ep-small"></view>
            <view class="band-block">
                <image class="bb bb-1" mode="widthFix" src="https://img.zhichiwangluo.com/zcimgdir/album/file_5ca30ed3589d5.png"><text class="bb-title">网站建设</text><text class="bb-subtitle">行业门户、电商、物联网等响应式网站</text>
                </image>
                <image class="bb bb-2" mode="widthFix" src="https://img.zhichiwangluo.com/zcimgdir/album/file_5ca30f149cc3d.png"><text class="bb-title">H5活动</text><text class="bb-subtitle">活动营销、产品推荐</text>
                </image>
                <image class="bb bb-3" mode="widthFix" src="https://img.zhichiwangluo.com/zcimgdir/album/file_5ca30f0cbaf66.png"><text class="bb-title">移动开发</text><text class="bb-subtitle">电商类、医疗类、旅游类、社交类等</text>
                </image>
                <image class="bb bb-4" mode="widthFix" src="https://img.zhichiwangluo.com/zcimgdir/album/file_5ca30f324f13e.png"><text class="bb-title">小程序</text><text class="bb-subtitle">微信、支付宝、百度小程序</text>
                </image>
            </view>
        </view>
        <view class="empty-padding"></view>
        <view class="block" id="success">
            <view class="block-head">
                <view class="iff-icon left"></view><text class="bh-word">成功案例</text>
                <view class="iff-icon right"></view>
            </view>
            <view class="empty-padding ep-small"></view>
            <view class="success-block">
                <image bindtap="skipToExcise" class="sb" mode="scaleToFill" src="http://fame.xiangxinjs.com/static/images/banner2.jpg"><text class="sb-title">乾承周易文化</text>
                </image>
                <image bindtap="skipToEmall" class="sb" mode="scaleToFill" src="http://yanxuan.nosdn.127.net/65091eebc48899298171c2eb6696fe27.jpg"><text class="sb-title">宇云商城</text>
                </image>
            </view>
        </view>
        <view class="empty-padding"></view>
        <view class="block" id="contact">
            <view class="block-head">
                <view class="iff-icon left"></view><text class="bh-word">联系我们</text>
                <view class="iff-icon right"></view>
            </view>
            <view class="empty-padding ep-small"></view>
            <view class="contact-block"><text class="cb-word1">地址  |  Address</text>
                <view class="cb-word2">上海市嘉定区嘉行公路3188号8幢J274室</view>
                <view class="sb-icon icon-address"></view>
            </view>
            <view class="contact-block" bindtap="callingPhone"><text class="cb-word1">电话  |  Telephone</text>
                <view class="cb-word2">17301631743</view>
                <view class="sb-icon icon-phone"></view>
            </view>
        </view>
        <view class="float-phone" bindtap="callingPhone"></view>
        <view class="company-ties" bindtap="skipToPcHome">由<text class="ct-name">湘昕科技</text>提供技术服务支持</view>
    </scroll-view>
</block>